; vim: ft=asm

	a
	b
	c
	d

	; this doesn't really change anything
	add %reg0 %reg1

	move 10 %reg0
	move 15 %reg1
	add %reg0 %reg1
	; at this point, %reg1=15

	move %reg1 a
	; at this point, a=15

	inc %reg1
	inc a
	; at this point, a=16 and %reg1=16

	move a %reg0
	sub %reg1 %reg0
	; at this point, %reg0=0 and %reg1 is still 16
